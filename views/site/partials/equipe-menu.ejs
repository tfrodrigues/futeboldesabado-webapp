<script type="text/javascript">
	function abreMenuTimes(menu) {
		$("[data-js='menu-times']").removeClass("is-active");
		$("[data-menu='"+menu+"']").addClass("is-active");
		$("[data-js='submenu']").removeClass("is-active");
		$("[data-submenu='"+menu+"']").addClass("is-active");
	}

	function abreLetra(letra) {
		$("[data-js='letra']").removeClass("is-active");
		$("[data-letra='"+letra+"']").addClass("is-active");
		$("[data-js='timesLetra']").removeClass("is-active");
		$("[data-timesLetra='"+letra+"']").addClass("is-active");
	}

	function fechaMenuTimes() {
		$("[data-js='menu-times']").removeClass("is-active");
		$("[data-js='submenu']").removeClass("is-active");
	}
</script>
